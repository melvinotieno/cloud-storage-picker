{
  "name": "cloud-storage-picker",
  "version": "0.0.0",
  "description": "Pick files from a cloud storage provider such as Dropbox, Google Drive and more.",
  "keywords": [
    "dropbox-chooser",
    "google-drive-picker"
  ],
  "homepage": "https://github.com/melvinotieno/cloud-storage-picker",
  "bugs": "https://github.com/melvinotieno/cloud-storage-picker/issues",
  "license": "MIT",
  "author": "Melvin Otieno <o.melvinotieno@gmail.com> (https://melvinotieno.com)",
  "repository": {
    "type": "git",
    "url": "https://github.com/melvinotieno/cloud-storage-picker"
  },
  "engines": {
    "node": ">=22.14.0",
    "pnpm": ">=10.28.0"
  },
  "packageManager": "pnpm@10.28.1",
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./dropbox": "./dist/providers/dropbox.js",
    "./google-drive": "./dist/providers/google-drive.js"
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch --onSuccess \"http-server\""
  },
  "devDependencies": {
    "@types/dropbox-chooser": "^1.0.8",
    "@types/gapi": "^0.0.47",
    "@types/google.accounts": "^0.0.18",
    "@types/google.picker": "^0.0.52",
    "http-server": "^14.1.1",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  }
}
